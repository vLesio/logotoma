string pattern switch_on_off(int a){
    if(a > 0){
        return "True"
    }
    else{
        return "False"
    }
}

int r = 128
int g = 0
int b = 128
bool x = False
spray x
engine True
sleep 20
engine off
x = cast switch_on_off(1) -> bool
spray x
float spray_size = 2.0
loop 425 times{
    engine on
    wheel left
    sleep 10
    engine off
    wheel off
    r = r + 1
    b = b - 1
    spray color (r, g, b)
    if(r % 10 <= 5){
        spray_size = spray_size + 0.1
    }
    else{
       spray_size = spray_size - 0.1
    }
    spray size spray_size
}
spray cast switch_on_off(-1) -> bool
hide on
sleep cast "100" -> int
save "picture"
