bool pattern switch_off(){
    return False
}
bool pattern switch_on(){
    return True
}

int r = 128
int g = 0
int b = 128
bool x = False
spray x
engine True
sleep 20
engine off
x = switch_on()
spray x
float spray_size = 2.0
loop 425 times{
    engine on
    wheel left
    sleep 10
    engine off
    wheel off
    r = r + 1
    b = b - 1
    spray color (r, g, b)
    if(r % 10 <= 5){
        spray_size = spray_size + 0.1
    }
    else{
       spray_size = spray_size - 0.1
    }
    spray size spray_size
}
spray switch_off()
sleep 250
