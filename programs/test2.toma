number pattern square(number x_start, number y_start, number x_end, number y_end){
    line x_start y_start x_start y_end
    line x_start y_end x_end y_end
    line x_end y_end x_end y_start
    line x_end y_start x_start y_start}
number house_size_x = 20
number house_size_y = 20
# Obramowka domu
brush-color (255, 0, 0, 100)
move 5 5
square(position_x, position_y, position_x, position_y)
# Daszek
move #house_size_x (5+5*5)*(5*(6+4)*69)+10
line position_x position_y position_x position_y/2
bool small_house = True
# Jezeli domek maly narysuj drzwi po lewej jezeli nie to na srodku
move #5 5
if (small_house == x < y){
    move #3 #0
}
else{
    move #house_size_x #0
}
square(position_x, position_y, position_x, position_y)
move 5 5
# Jezeli domek maly narysuj okno po prawej jezeli nie to dwa okna po obu stronach drzwi
if(small_house == False){
    circle 10 house_size_y 3
}
circle house_size_x house_size_y 3
