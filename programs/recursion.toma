int pattern silnia(int x){
    if(x < 0){
        print("argument musi byÄ‡ nieujemny")
        return -1
    }
    if (x == 0){
        return 1
    }
    else{
        return x * silnia(x - 1)
    }
}

int pattern counter(int a){
    if (a < 10){
        a = a + 1
        return counter(a)
    }
    return a
}

print(counter(1))
print(counter(11))
print(silnia(-1))
print(silnia(0))
print(silnia(6))